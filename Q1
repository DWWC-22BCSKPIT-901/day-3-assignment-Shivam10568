#include <iostream>
using namespace std;
int sumArray(int arr[], int n) {
 if (n == 0) return 0;
 return arr[n - 1] + sumArray(arr, n - 1);
}
int main() {
 int n;
Name:- Shivam Aanand UID:- 22BCS10568 Section:- 22KPIT_901(A)
 cin >> n;
 int arr[n];
 for (int i = 0; i < n; i++) {
 cin >> arr[i];
 }
 cout << sumArray(arr, n);
 return 0;
}
